import java.util.Scanner;

public class Lab2_14_2 {

	static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		String input;
		input = sc.nextLine();
	
		for (char i = 'A'; i <= 'Z'; i++) {
			
			if (input.substring(0).equals(i)||input.substring(1).equals(i)) {
				System.out.print(i);
			}

		}
	}

}
