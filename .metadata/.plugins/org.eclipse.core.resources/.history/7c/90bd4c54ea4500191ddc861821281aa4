import java.util.Scanner;

public class Lab1_11_4 {
	
	static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		float coust = 0;
		float profit = 0;
		System.out.println("Coust :");
		coust = Math.round(Float.parseFloat(sc.nextLine()));
		System.out.println("Profot :");
		profit = Math.round(Float.parseFloat(sc.nextLine()));
		System.out.println("Sale Price ="+((profit/100)*coust)+coust);

	}

}
