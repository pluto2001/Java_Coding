import java.util.Scanner;

public class Lab2_14_1 {

	static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		String input;
		input = sc.nextLine();
		for (char i = 'A'; i <= 'Z'; i++) {
			//System.out.println(i);
			input = sc.nextLine();
			System.out.println(input.substring(i));
		}

	}

}
