
public class Hello {
	// boolean		--> true, false
	// char
	// byte       	--> int
	// shot		  	--> int
	// int	
	// long			--> int
	// float		
	// double		--> float
	// String		--> Text

	public static void main(String[] args) {
		float t = (float) 10.6;
		int a = Math.round(t);
		System.out.println(a);
		
	}


}
