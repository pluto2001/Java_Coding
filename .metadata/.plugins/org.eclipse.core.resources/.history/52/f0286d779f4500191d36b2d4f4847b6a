import java.util.Scanner;

public class Hello {
	// boolean		--> true, false
	// char
	// byte       	--> int
	// shot		  	--> int
	// int	
	// long			--> int
	// float		
	// double		--> float
	// String		--> Text
	
	static Scanner sc = new Scanner(System.in);
	public static void main(String[] args) {
		System.out.println(sc.nextInt());
		
	}


}
