import java.util.Scanner;

public class Lab1_11_5 {
	
	static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		float ab = 0;
		float p = 3.1428f;
		System.out.println("AB = ");
		ab = Math.round(Float.parseFloat(sc.nextLine()));
		System.out.println(ab*ab);
		System.out.println(p*(ab/2)*(ab/2));
		System.out.println((ab*ab)-(p*(ab/2)*(ab/2))*0.25);
		System.out.println(p*(ab/2)*(ab/2)*0.25);
		System.out.print("Area");
		System.out.println((ab*ab)-(p*(ab/2)*(ab/2))*0.25+p*(ab/2)*(ab/2)*0.25);

	}

}
